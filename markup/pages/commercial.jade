    .step
        .container
            .step__title Как заказать отчёт об оценке?
        .step__greenBuffer
        .step__green
            .container
                .step__content
                    ul.step__list
                        li
                            .step__item
                                .step__item-icon
                                    .step__item-icon-num 1
                                    +img('content/step/1', 'png')
                                .step__item-title Оставьте заявку
                                .step__item-text На сайте или по #[br]телефону #[a(href='#').color-blue: span.color-blue__title.color-blue__title_viewBorder +7 495 721 18 41]
                        li
                            .step__item
                                .step__item-icon
                                    .step__item-icon-num 2
                                    +img('content/step/2', 'png')
                                .step__item-title Предоставьте #[br]документы
                                .step__item-text Электронные копии, #[br]предоставленные по эл. почте#[br]#[a(href='#').color-blue: span.color-blue__title.color-blue__title_viewBorder ocenka-74@mail.ru]
                        li
                            .step__item
                                .step__item-icon
                                    .step__item-icon-num 3
                                    +img('content/step/3', 'png')
                                .step__item-title Оплата
                                .step__item-text Оплата онлайн или #[br]наличными в офисе
                        li
                            .step__item
                                .step__item-icon
                                    .step__item-icon-num 4
                                    +img('content/step/4', 'png')
                                .step__item-title Осмотр #[br](при необходимости)
                                .step__item-text В будни и в выходные #[br]09:00 до 18:00
                        li
                            .step__item
                                .step__item-icon
                                    .step__item-icon-num 5
                                    +img('content/step/5', 'png')
                                .step__item-title Подготовка отчета
                                .step__item-text От 1 рабочего дня, после #[br]осмотра и предоставления #[br]всех документов
                        li
                            .step__item
                                .step__item-icon
                                    .step__item-icon-num.step__item-icon-num_bgGreen 6
                                    +img('content/step/6', 'png')
                                .step__item-title Доставка
