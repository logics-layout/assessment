extends main

block head
    - var headData = {title: "Коммерческая недвижимость"}
    link(href='%=static=%css/separate-css/slick.css', rel="stylesheet", type="text/css")

block content
    .content
        .container
            //.hidden-md-down
                br

            h2 Выберите тип недвижимости:

            .service-tabs-list.not-styleLink
                .service-tabs-list__section
                    a(href="#").service-tabs-list__item
